{
  "name": "atom-mdtoc",
  "main": "./lib/atom-mdtoc",
  "version": "0.8.5",
  "description": "Generate a markdown TOC (table of contents) in atom editor using Markdown-TOC and Remarkable from Jon Schlinkert.",
  "keywords": [
    "markdown",
    "toc",
    "atom"
  ],
  "activationCommands": {
    "atom-workspace": [
      "atom-mdtoc:insert",
      "atom-mdtoc:delete"
    ]
  },
  "activationHooks": [
    "language-gfm:grammar-used",
    "language-gfm-enhanced:grammar-used",
    "language-markdown:grammar-used"
  ],
  "repository": "https://github.com/mcpride/atom-mdtoc",
  "author": "mcpride (Marco Stolze)",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "markdown-toc": "^0.12.11"
  }
}
